{
    "compilerOptions": {
      "module": "NodeNext", 
      "moduleResolution": "NodeNext",
      "target": "ESNext",
      "strict": true,
      "skipLibCheck": true, 
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist"
    },
  
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }